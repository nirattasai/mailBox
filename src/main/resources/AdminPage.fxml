<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="scene" prefHeight="600.0" prefWidth="1000.0" styleClass="background" stylesheets="@css/admin.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Final.Controller.AdminController.AdminPageController">
   <children>
      <Pane id="pane" prefHeight="600.0" prefWidth="200.0" stylesheets="@css/admin.css">
         <children>
            <Label id="label" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" text="ADMIN" textFill="WHITE">
               <font>
                  <Font name="Helvetica" size="37.0" />
               </font>
            </Label>
            <Label id="button" fx:id="addStaffButton" alignment="CENTER" layoutY="200.0" onMouseClicked="#handleAddStaffButton" prefHeight="40.0" prefWidth="200.0" text="ADD STAFF" />
            <Label id="button" fx:id="checkStaffButton" alignment="CENTER" layoutY="240.0" onMouseClicked="#handleCheckStaffButton" prefHeight="40.0" prefWidth="200.0" text="CHECK STAFF" />
         </children>
      </Pane>
      <Label id="button" fx:id="changePasswordButton" alignment="CENTER" layoutY="280.0" onMouseClicked="#handleChangePasswordButton" prefHeight="40.0" prefWidth="200.0" text="CHANGE PASSWORD" />
      <Label id="button" fx:id="logoutButton" alignment="CENTER" layoutY="560.0" onMouseClicked="#handleLogoutButton" prefHeight="40.0" prefWidth="200.0" text="LOGOUT" />
      <TableView fx:id="staffTableView" layoutX="250.0" layoutY="25.0" onMouseEntered="#reset" prefHeight="550.0" prefWidth="600.0" styleClass="table-column" />
      <Button id="glass" layoutX="898.0" layoutY="28.0" mnemonicParsing="false" onAction="#handleCheckButton" stylesheets="@css/stylesheet.css" text="CHECK" />
      <Button id="glass" layoutX="903.0" layoutY="84.0" mnemonicParsing="false" onAction="#handleSortButton" stylesheets="@css/stylesheet.css" text="SORT" />
   </children>
</AnchorPane>
